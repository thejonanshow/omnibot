# Environment variables template
# Copy this to .env and fill in your actual values
# NEVER commit .env to git!

# Cloudflare
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_API_TOKEN=your_api_token_here

# LLM API Keys
ANTHROPIC_API_KEY=sk-ant-api03-...
OPENAI_API_KEY=sk-proj-...
GROQ_API_KEY=gsk_...
GEMINI_API_KEY=AIza...

# Runloop (Optional - for voice services)
RUNLOOP_API_KEY=ak_...
RUNLOOP_DEVOX_ID=dbx_...
RUNLOOP_URL=https://...

# Security
SHARED_SECRET=generate_a_secure_random_string_here

# GitHub (Optional - for self-upgrade feature)
GITHUB_TOKEN=ghp_...
GITHUB_REPO=your_username/omnibot
GITHUB_BRANCH=main
